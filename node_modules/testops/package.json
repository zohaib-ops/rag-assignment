{
  "name": "testops",
  "version": "1.0.0-alpha.2",
  "description": "TestOps NodeJS SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "node tests/run_e2e_tests.mjs",
    "test:e2e:basic": "node tests/test_tracing_basic.mjs",
    "test:e2e:operations": "node tests/test_tracing_operations.mjs",
    "test:e2e:datasets": "node tests/test_dataset_runs.mjs",
    "test:e2e:experiments": "node tests/test_experiment_tracing.mjs",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "dev": "ts-node src/index.ts"
  },
  "keywords": [
    "testops",
    "testing",
    "observability",
    "tracing",
    "nodejs",
    "typescript"
  ],
  "dependencies": {
    "langfuse": "3.38.5",
    "testops": "https://static.testops.ai/sdk/nodejs/testops-1.0.0a2.tar.gz"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "dotenv": "^16.3.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "openai": "^5.23.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.2.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    ".npmignore"
  ],
  "engines": {
    "node": ">=20"
  }
}
